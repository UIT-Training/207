	//发送第一次请求
	private void sendLoginInfoToServer(Object o) {
		try {
			Socket s =new Socket("127.0.0.1",9999);
			ObjectOutputStream oos=new  ObjectOutputStream(s.getOutputStream());
			oos.writeObject(o);
			
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
